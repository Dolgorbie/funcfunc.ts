{
  "name": "funcfunc",
  "version": "0.0.1",
  "description": "functional programing framework",
  "type": "module",
  "main": "./dist/main.js",
  "exports": {
    ".": {
      "default": "./dist/main.js",
      "import": "./dist/main.js",
